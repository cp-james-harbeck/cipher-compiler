{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", ".next", "*.d.ts", "public/**/*", "node_modules/**/*", "node_modules"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "node/no-deprecated-api": "off"
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
